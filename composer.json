{
    "name": "softspring/dynamic-form-type",
    "description": "This component allows to create dynamic forms from an array configuration",
    "type": "symfony-library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "symfony/form": "^5.4|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "symfony/validator": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DynamicFormType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DynamicFormType\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev"
        }
    }
}
