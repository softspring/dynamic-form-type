{
    "name": "softspring/dynamic-form-type",
    "description": "This component allows to create dynamic forms from an array configuration",
    "type": "symfony-library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/form": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.49.*",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "symfony/validator": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DynamicFormType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DynamicFormType\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
